simu <- function(n){
k <- 0
for(i in 1:n) {
a <- sample(y,1,replace=TRUE)
b <- sample(y,1,replace=TRUE)
c <- sample(y,1,replace=TRUE)
if ((a + b + c) > 12)  {
k <- k+1
} else if ((a + b + c) < 6)  {
k <- k-1
} else if ((a == b) & (b == c))  {
k <- k+5
} else k <- 0
}
return(k)
}
